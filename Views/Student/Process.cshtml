@model IEnumerable<PlagChecker.Models.Result>

@{
    ViewBag.Title = "Process";
}

<h2>Process</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Studen_RollNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Plag_RollNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PlagPercentage)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Studen_RollNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Plag_RollNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PlagPercentage)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ResultID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ResultID }) |
